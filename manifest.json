{
  "manifest_version": 3,
  "name": "Translate Lablekey",
  "version": "1.0",
  "description": "Reads data from element with ID 'lablekey' and translates it.",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["https://translate.googleapis.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}

